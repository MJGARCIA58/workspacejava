

public class ActividadEstudiantes {

	//Atributos;
	
		int id;
		int edad;
		int telefono;
		int numeroDeNotas;
		double sumaDeNotas;
		double notaMedia;
	
	//constructor parametrizado
		public ActividadEstudiantes(int id, int edad, int telefono, int numeroDeNotas, double sumaDeNotas, double notaMedia) {
			this.id = id;
			this.edad = edad;
			this.telefono = telefono;
			this.numeroDeNotas = numeroDeNotas;
			this.sumaDeNotas = sumaDeNotas;
			this.notaMedia = notaMedia;
		}
		
		// cliqueo en el boton derecho, source y le damos generateusing fields, y te crea solo el constructor 
		
		
	
	
	//Métodos
	void mostrarInfo(){}
	public ActividadEstudiantes(int id, int edad) {
			super();
			this.id = id;
			this.edad = edad;
		}

	void agregarNuevaNota() {}
	
	}

	
