package curso;

public class Profesor {
	//Atributos
		protected float numeroDeEdadesEstudiantes;
		protected int sumaDeEdadesEstudiantes;
		protected float estudiantesEdadMedia;

		//constructor
		public Profesor(float numeroDeEdadesEstudiantes, int sumaDeEdadesEstudiantes, float estudiantesEdadMedia) {
			super();
			this.numeroDeEdadesEstudiantes = numeroDeEdadesEstudiantes;
			this.sumaDeEdadesEstudiantes = sumaDeEdadesEstudiantes;
			this.estudiantesEdadMedia = estudiantesEdadMedia;
		}
		
		// metodos publicos no estáticos
			public void agregarEdadEstudiante() {
				System.out.println("Método agregarEdadEstudiante de Profesor");
			}
	
			public void mostrarInfo() {
				
			}
			
			//Get y set
			public float getNumeroDeEdadesEstudiantes() {
				return numeroDeEdadesEstudiantes;
			}
			public void setNumeroDeEdadesEstudiantes(float numeroDeEdadesEstudiantes) {
				this.numeroDeEdadesEstudiantes = numeroDeEdadesEstudiantes;
			}
			public int getSumaDeEdadesEstudiantes() {
				return sumaDeEdadesEstudiantes;
			}
			public void setSumaDeEdadesEstudiantes(int sumaDeEdadesEstudiantes) {
				this.sumaDeEdadesEstudiantes = sumaDeEdadesEstudiantes;
			}
			public float getEstudiantesEdadMedia() {
				return estudiantesEdadMedia;
			}
			public void setEstudiantesEdadMedia(float estudiantesEdadMedia) {
				this.estudiantesEdadMedia = estudiantesEdadMedia;
			}

		

}
